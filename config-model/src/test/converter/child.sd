search child {

  document child inherits parent {

    field a type uri {
       indexing: index | summary
    }

    field r type redef {
      indexing: summary
    }

    field aaa type annotationreference<gpa> { }

    field modelref type reference<other> { }

  }
  field outrarr type array<string> {
      indexing: input a | to_array | summary
  }

}
