schema predicate_type {
    document predicate_type {
        field some_predicate_field type predicate {
            indexing: attribute | summary
            index {
                arity: 5
                lower-bound: 3
                upper-bound: 200
                dense-posting-list-threshold: 0.2
            }
        }
    }
}
