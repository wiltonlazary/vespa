schema arrays {

  document arrays {

    field tags type array<string> {
      indexing: summary | index
    }

    field ratings type int[] {
      indexing: summary | index
    }

    field a type string {
      indexing: index
    }

    field b type array<string> {
      indexing: index  | attribute
    }

    field c type weightedset<string> {
      indexing: summary | index
    }

  }

  fieldset default {
      fields: a, b, c
  }

}
