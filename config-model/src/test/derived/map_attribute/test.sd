schema test {
  document test {
    field str_map type map<string, string> {
      indexing: summary 
      struct-field key {
        indexing: attribute
        attribute: fast-search
      }
      struct-field value {
        indexing: attribute
      }
    }  
    field int_map type map<int, int> {
      indexing: summary 
      struct-field key {
        indexing: attribute
      }
    }  
  }
}
