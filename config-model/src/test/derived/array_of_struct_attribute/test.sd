schema test {
  document test {
    struct elem {
      field name type string {}
      field weight type int {}
    }
    field elem_array type array<elem> {
      indexing: summary
      struct-field name {
        indexing: attribute
        attribute: fast-search
      }
      struct-field weight {
        indexing: attribute
      }
    }
  }
  document-summary rename {
    summary new_elem_array type array<elem> { source: elem_array }
  }
}
