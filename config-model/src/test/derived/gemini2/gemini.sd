schema gemini {

    document gemini {
        field right type string {
            indexing: attribute
        }
        field wrong type string {
            indexing: attribute
        }
    }

    rank-profile test {

        function wrapper2(x) {
            expression: x
        }

		function wrapper1(x) {
			expression: wrapper2(x)
		}

        function toplevel() {
            expression: wrapper1(attribute(right))
        }

        function interfering() {
            expression: wrapper1(attribute(wrong))
        }

    }

}
