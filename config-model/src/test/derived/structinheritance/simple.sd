schema simple {
  document simple {
    struct base {
      field name type string {}
    }
    struct child inherits base {
      field age type int {}
    }
    struct grandchild inherits child {
      field toy type string {}
    }
    field f1 type child {}
    field f2 type grandchild {}
  }
}
